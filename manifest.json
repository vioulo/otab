{
    "manifest_version": 2,
    "name": "Otab",
    "version": "0.2.0",
    "browser_specific_settings": {
        "gecko": {
            "id": "firefox-otab@void.gold",
            "strict_min_version": "100.0"
        }
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "icons": {
        "32": "imgs/icons/icon32.png",
        "48": "imgs/icons/icon48.png",
        "96": "imgs/icons/icon96.png",
        "128": "imgs/icons/icon96.png"
    },
    "chrome_url_overrides": {
        "newtab": "pages/new-tab.html"
    },
    "chrome_settings_overrides" : {
        "homepage": "pages/new-tab.html"
    },
    "options_ui": {
        "page": "pages/setting.html",
        "open_in_tab": true
    },
    "permissions": [
        "bookmarks",
        "storage"
    ]
}